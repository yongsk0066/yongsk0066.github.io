{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "useDefineForClassFields": false,
    "experimentalDecorators": true,
    "baseUrl": "/Users/yongseok/dev/yongsk0066.github.io",
    "paths": {
      "@styles/*": [
        "src/styles/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@consts": [
        "src/consts"
      ],
      "@*": ["./src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}